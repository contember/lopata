{
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".react-router/types/**/*"
	],
	"compilerOptions": {
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"isolatedModules": true,
		"rootDirs": [".", "./.react-router/types"],
		"types": ["@cloudflare/workers-types/2023-07-01"]
	}
}
